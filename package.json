  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      ".+\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "preset": "ts-jest",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.tsx?$": "babel-jest",
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.mdx?$": "@storybook/addon-docs/jest-transform-mdx"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/Stories/**",
      "!**/mocks/**",
      "!**/coverage/**",
      "!jest.config.js",
      "!**/generateSitemap/processWrapper.tsx",
      "!**/pages/_app.tsx"
